n.\rpieza setNameTo:\'TorreNegro\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\ri:=1.\r[i<=8]\rwhileTrue:[\rpieza:=(PeonV new) setColor.\rcasilla:= tableroVisual at:7 at:i.\r(tableroVisual at:7 at:i) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rcasilla addMorph: pieza.\rpieza setNameTo:(\'PeonNegro\',i).\rpieza position: origen  + (5@5).\r\rcol add: pieza.\ri:=i+1].\r\r"PIEZAS BLANCAS"\r\rpieza:=(TorreV new).\rcasilla:= tableroVisual at:1 at:1.\r(tableroVisual at:1 at:1) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'TorreBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(CaballoV new).\rcasilla:= tableroVisual at:1 at:2.\r(tableroVisual at:1 at:2) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'CaballoBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(AlfilV new).\rcasilla:= tableroVisual at:1 at:3.\r(tableroVisual at:1 at:3) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'AlfilBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(ReinaV new).\rcasilla:= tableroVisual at:1 at:4.\r(tableroVisual at:1 at:4) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'ReinaBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(ReyV new).\rcasilla:= tableroVisual at:1 at:5.\r(tableroVisual at:1 at:5) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'ReyBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(AlfilV new).\rcasilla:= tableroVisual at:1 at:6.\r(tableroVisual at:1 at:6) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'AlfilBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(CaballoV new).\rcasilla:= tableroVisual at:1 at:7.\r(tableroVisual at:1 at:7) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'CaballoBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\rpieza:=(TorreV new).\rcasilla:= tableroVisual at:1 at:8.\r(tableroVisual at:1 at:8) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:\'TorreBlanco\'.\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\r\rcol add: pieza.\r\ri:=1.\r[i<=8]\rwhileTrue:[\rpieza:=(PeonV new).\rcasilla:= tableroVisual at:2 at:i.\r(tableroVisual at:2 at:i) addDependent: pieza.\rorigen:= casilla innerBounds origin.\rpieza setNameTo:(\'PeonBlanco\',i).\rcasilla addMorph: pieza.\rpieza position: origen  + (5@5).\rcol add: pieza.\ri:=i+1].',			#stamp : 'mp 7/3/2016 21:49',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:47.864901-03:00' ],		#prior : OmReference [ '41' ],		#self : OmReference [ '42' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #JuegoVisual,				#isMetaSide : false			},			#name : #agregarVertical,			#protocol : #'as yet unclassified',			#sourceCode : 'agregarVertical\r| cuadrado cont y tabla |\r\r"Agrega referencia a filas"\rtabla:= Morph new.\r\rtabla color:Color transparent.. \rtabla position: 50@50.\rtabla bounds: (self position extent:  75@550).\rtabla layoutPolicy: TableLayout new.\rtabla listDirection: #topToBottom.\rtabla listCentering: #bottomRight.\rtabla wrapCentering: #topLeft.\rtabla listCentering: #topLeft.\rtabla borderWidth: 1.\r\rcont:=8.\r\r[cont>=1]whileTrue:\r[\rcuadrado := BorderedMorph new.\rcuadrado color:Color transparent.\rcuadrado borderColor:Color transparent. \rcuadrado layoutPolicy: TableLayout new.\rcuadrado listCentering: #center. "wrapCentering: #center."\ry:= StringMorph new contents:\'\',cont.\rcuadrado addMorph: y.\rcuadrado bounds: (cuadrado position extent:  65@65).\rtabla addMorph: cuadrado.\rcont:=cont-1.].\r^tabla',			#stamp : 'mp 7/26/2016 19:22',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:47.906901-03:00' ],		#prior : OmReference [ '42' ],		#self : OmReference [ '43' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #JuegoVisual,				#isMetaSide : false			},			#name : #initialize,			#protocol : #'as yet unclassified',			#sourceCode : 'initialize\r|origen filas columnas  width height sampleCell j1 j2 nombre1 nombre2 vertical horizontal|\r\rsuper initialize.\rcolCasillas:=OrderedCollection new.\rself color:(Color r:0.658 g:0.658 b:0.662).\rself position: 50@50.\r\rfilas :=8.\rcolumnas := 8.\r\rsampleCell:=Casilla new.\rwidth:= sampleCell width.\rheight:=sampleCell height.\r\rnombre1:=FillInTheBlank request: \'Nombre Jugador 1\'.\rnombre2:=FillInTheBlank request: \'Nombre Jugador 2\'.\r\rj1:=Jugador crearJugadorNombre:nombre1 color:\'Blanco\'.\rj2:=Jugador crearJugadorNombre:nombre2 color:\'Negro\'.\r\r"Creamos el juego"\rmiJuego:= Juego crearJuegoJugador1:j1 jugador2:j2. \r"Obtenemos el tablero del juego"\rtablero:= miJuego getTablero.\r\r"Empieza jugando el jugador blanco"\rturno:=false.\r\rtableroVisual:= Matrix rows: filas columns: columnas tabulate: [ :i :j | self nuevaCeldaFila: i columna: j].\r\rself bounds: (self position extent: (columnas * width +610)@(filas * height + 550)).\r\r"Agregamos las piezas visuales al tablero visual"\rself agregarPiezas.\r\rbotonCerrar:= BotonAccion new.\rbotonJugador1:= BotonAccion new.\rbotonJugador2:= BotonAccion new.\r\rbotonJugador1 color:Color white.\rbotonJugador2 color:Color black.\r\r\rself addMorph: botonCerrar.\rself addMorph: botonJugador1.\rself addMorph: botonJugador2.\r\r\rorigen := self innerBounds origin.\rbotonCerrar position: (681@55).\rbotonJugador1 position: (620@77).\rbotonJugador2 position: (620@577).\rbotonJugador1 label:\'J1: \',nombre1.\rbotonJugador2 label:\'J2: \',nombre2.\r(botonJugador2 firstSubmorph) color:Color white.\r\rbotonCerrar label:\'CERRAR\'.\r\rbotonCerrar mouseAction: [self abandon].\r\r"Agregamos el eje vertical"\rvertical:= self agregarVertical.\rself addMorph: vertical.\rvertical position:  (600@74).\r\r"Agregamos el eje horizontal"\rhorizontal:= self agregarHorizontal.\rself addMorph: horizontal.\rhorizontal position:  (69@605).',			#stamp : 'mp 7/26/2016 19:41',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:47.948901-03:00' ],		#prior : OmReference [ '43' ],		#self : OmReference [ '44' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #JuegoVisual,				#isMetaSide : false			},			#name : #'nuevaCeldaFila:columna:',			#protocol : #'as yet unclassified',			#sourceCode : 'nuevaCeldaFila: i columna: j\r| p c origen coor|\r\r"Crea una casilla para la posición (i,j) y la agrega al tableroVisual."\rc := CasillaVisual new.\rp:= PiezaVisual new.\r"Se toma el punto superior izquierdo de la instancia de JuegoVisual (self) para a partir de alli ubicar a las casillas visuales"\rorigen := self innerBounds origin.\r\rcoor:= Array new:2.\rcoor at:1 put:i.\rcoor at:2 put:j.\r\r"Pone el color correspondiente a cada casilla"\r((i+j)\\\\2=0)\rifTrue:[c color:Color white]\rifFalse:[c color:Color black].\r"A cada casilla visual le seteamos como nombre su coordenada"\rc setNameTo: coor.\rc setCoordenada: coor.\r\r"Se agrega la casilla visual creada al morph contenedor, en este caso la instancia de JuegoVisual, y se posiciona en la pantalla"\rself addMorph:c.\r\r"Se realiza la conexion entre la casilla visual y la casilla del modelo, la casilla visual se agrega como dependiente del modelo."\r(tablero fila: i columna:j) addDependent: c.\r\rc position: ((j - 1) * (c width )) @ ((i - 1) * (c height )) + origen + (20@25).\r\r"Se define la acción que se ejecutara cuando se accione sobre la casilla visual"\rc mouseAction:[\r\t\t\t\t\t\r\r\t\t\t\t\t((c hasSubmorphs)&(c borderColor~=Color red)&(unaPieza=nil)&((miJuego getJugadas)<=(miJuego getCantMaxJugadas)))\r\t\t\t\t\t\t\t\t\t ifTrue:\t[\r\t\t\t\t\t\t\t\t\t\t\t piezaVisual := c firstSubmorph. "Almacenamos la pieza visual seleccionada"\r\t\t\t\t\t\t\t\t\t\t\t\r\t\t\t\t\t\t\t\t\t\t\t unaPieza:=(tablero fila: i columna:j)getPieza. "Almacenamos la pieza del modelo"\r\t\t\t\t\t\t\t\t\t\t\t\t ((((unaPieza getColor)=\'Blanco\')&(turno=false))|(((unaPieza getColor)=\'Negro\')&(turno=true)))\r\t\t\t\t\t\t\t\t\t\t\t\t ifTrue:\t[colCasillas:=((tablero fila: i columna:j)getPieza) posiblesMovimientos:tablero.\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t (colCasillas isEmpty) ifTrue:[UIManager inform:\'No tiene movimientos\'. unaPieza:=nil. piezaVisual:=nil].\t\t\t\t\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t]\r\r\t\t\t\t\t\t\t\t\t\t\t\t ifFalse: [unaPieza:=nil . piezaVisual:=nil. UIManager inform: \'Turno Incorrecto\'].\r\t\t\t\t\t\t\t\t\t\t\t \r\t\t\t\t\t\t\t\t\t\t\t]\r\t\t\t\t\t\t\t\t\tifFalse:\t[\r\t\t\t\t\t\t\t\t\t\t\t(((miJuego getJugadas)>=(miJuego getCantMaxJugadas)) | (reyEliminado=true))\r\t\t\t\t\t\t\t\t\t\t\tifTrue:\t[UIManager inform: \'JUEGO TERMINADO\']\t\r\t\t\t\t\t\t\t\t\t\t\tifFalse:\t[\t\r\t\t\t\t\t\t\t\t\t\t\t\t\t((c borderColor = Color red) &(unaPieza~=nil))\t\t\t\t\t\t\t\t\t\t\t\r\t\t\t\t\t\t\t\t\t\t\t\t\tifTrue:\t[\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t((((tablero fila: i columna:j)getPieza)className)=\'Rey\') "Verificamos si en la casilla destin esta el Rey"\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tifTrue: \t[ c removeAllMorphs. \r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tunaPieza moverPiezaA:(tablero fila:i columna:j) tablero: tablero. reyEliminado:=true. c addMorph:piezaVisual.\r\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(turno=false) "Finaliza el juego mostrando el ganador"\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tifTrue:[UIManager inform: \'JUEGO TERMINADO, Ganador BLANCO\']\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tifFalse:[UIManager inform: \'JUEGO TERMINADO, Ganador NEGRO\'].\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t]\t\t\t\t\t\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tifFalse:\t[\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t"Se mueve la pieza a la casilla destino"\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tc removeAllMorphs.\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tunaPieza moverPiezaA:(tablero fila:i columna:j) tablero: tablero.\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tmiJuego setJugadas:1.\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tc addMorph:piezaVisual.\r\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t"Camibia el turno"\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(turno=true) ifTrue:[turno:=false] ifFalse:[turno:=true].\t\r\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tunaPieza:=nil. piezaVisual:=nil.\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t].\r\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t]\t\r\t\t\t\t\t\t\t\t\t\t\t\t\tifFalse:[(unaPieza=nil)ifTrue:[UIManager inform: \'Casilla vacia\']ifFalse:[UIManager inform: \'Movimiento Incorrecto\'.unaPieza:=nil. piezaVisual:=nil]].\t\r\t\t\t\t\t\t\t\t\t\t\t\t\t].\tcolCasillas do:[:elem | elem desmarcar].\r\r\t\t\t\t\t\t\t\t\t\t\t].\r\t\t\t\t\t\r\t\t\t\t\t\t].\r\r^c.   ',			#stamp : 'mp 7/26/2016 19:42',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:48.006901-03:00' ],		#prior : OmReference [ '44' ],		#self : OmReference [ '45' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #JuegoVisual,				#isMetaSide : false			},			#name : #openInWorld,			#protocol : #'as yet unclassified',			#sourceCode : 'openInWorld\r"Por defecto el método openInWorld de la clase Morph inicia el step. Este método se redefine para desactivarlo\rhasta que sea explicitamente activado por el usuario con el boton correspondiente"\rsuper openInWorld.\r',			#stamp : 'mp 7/26/2016 19:48',			#package : #Ajedrez		}	}}