eza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla.\r\rcasilla:= matriz at:8 at:5.\rpieza:=(Rey crearPieza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla.\r\rcasilla:= matriz at:8 at:6.\rpieza:=(Alfil crearPieza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla.\r\rcasilla:= matriz at:8 at:7.\rpieza:=(Caballo crearPieza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla.\r\rcasilla:= matriz at:8 at:8.\rpieza:=(Torre crearPieza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla.\r\ri:=1.\r[i<=8]\rwhileTrue:[\rcasilla:= matriz at:7 at:i.\rpieza:=(Peon crearPieza:\'Negro\').\rcasilla setPieza:pieza.\rpieza setCasilla: casilla. i:=i+1.].',			#stamp : 'FV 7/1/2016 11:14',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:52.573901-03:00' ],		#prior : OmReference [ '128' ],		#self : OmReference [ '129' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #Tablero,				#isMetaSide : false			},			#name : #'estaOcupado:',			#protocol : #Aplicaciones,			#sourceCode : 'estaOcupado:coor\r|c|\r\rc:=colCasillas detect:[:elem | (elem getCoordenada = coor)]ifNone:[nil].\r\r((c~=nil))\rifTrue:[((c getPieza )=nil) ifTrue:[^false] ifFalse:[^true]]\rifFalse:[^true].\r',			#stamp : 'mp 7/1/2016 16:58',			#package : #Ajedrez		}	}}OmEntry {	#tags : {		#author : 'ManuelLatorre',		#time : DateAndTime [ '2019-08-05T20:10:53.401901-03:00' ],		#prior : OmReference [ '129' ],		#self : OmReference [ '130' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #Tablero,				#isMetaSide : false			},			#name : #'fila:columna:',			#protocol : #Aplicaciones,			#sourceCode : 'fila: f columna: c\r^matriz at: f at: c.',			#stamp : 'mp 6/28/2016 13:41',			#package : #Ajedrez		}	}}OmEntry